# Copy this file to .env and fill in your values.
# .env is .gitignore'd and never enters version control.

# Weights & Biases — for experiment tracking
WANDB_API_KEY=your-wandb-api-key

# Kaggle — for data download and dataset upload
KAGGLE_USERNAME=your-kaggle-username
KAGGLE_KEY=your-kaggle-api-key

# Kaggle dataset slug for model upload (used by upload_kaggle_dataset.sh)
KAGGLE_DATASET_SLUG=your-kaggle-username/deep-past-model

# RunPod SSH host — just the IP or hostname; scripts default to user "root"
# Example: RUNPOD_SSH_HOST=203.0.113.42  or  RUNPOD_SSH_HOST=user@203.0.113.42
RUNPOD_SSH_HOST=your-runpod-ip
